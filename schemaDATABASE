table Users
		=> id login active dateCreate
table UsersInfo
		=> uid avatar firstname lastname email
table UserPasswd
		=>uid pwd
table Posts
		=> post_id uid path likes dateCreate
table Comments
		=> comment_id uid post_id Comment likes dateCreate
table PostLikes
		=> like_id post_id uid dateCreate
table CommentLikes
		=> like_id comment_id uid dateCreate

